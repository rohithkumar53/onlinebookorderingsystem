export const orderPaymentReducer=(state={}, action)=>{
    const {type, payload}=action;
    switch(type){
        case "ORDER_PLACE_REQUEST":
            return {
                ...state,
                loading: true
            }
        case "ORDER_PLACE_SUCCESS":
            return {
                ...state,
                success: true,
                loading: false
            }
        case "ORDER_PLACE_FAILED":
            return {
                ...state,
                error: true,
                loading: false
                
            }
        default:
            return state
    }
}